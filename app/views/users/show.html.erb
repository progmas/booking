<h1>Profile</h1>

<h2>Your Events</h2>
<% @events.each do |event| %>
  <div>
    <h3><%= event.name %></h3>
    <p><%= event.description %></p>
    <p><%= event.location %></p>
    <p><%= event.date %></p>
  </div>
<% end %>

<h2>Your Tickets</h2>
<% @tickets.each do |ticket| %>
  <div>
    <h3><%= ticket.event.name %></h3>
    <p>Quantity: <%= ticket.quantity %></p>
  </div>
<% end %>